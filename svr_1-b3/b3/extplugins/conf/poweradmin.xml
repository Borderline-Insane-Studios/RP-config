<configuration plugin="poweradmin">
	<settings name="commands">
<!--  
		The first group of commands work with our serverside mod, or with bullet-worm's powerservermod. Or, in fact,
		with any server that has ravirs original admintools included.
    Whenever the coder modified the tools however, they might not work with b3.
-->
		<set name="paswitch-switch">100</set>
		<set name="paswitchall-switchall">100</set>
		<set name="paforcespec-fs">5</set>
		<set name="pakillplayer-killplayer">100</set>
<!--  
		The next commands only apply when you have installed the _b3_poweradmin.gsc as an extra serverside mod
		You have to know basic CoD scripting to add it to your gametype scripts! If you don't, leave it alone for now!
-->
		<set name="pafullbright-fps">0</set>
		<set name="pafps-yitch3">0</set>
		<set name="painvisible-tryhardmode">100</set>
		<set name="paforce-force">5</set>
		<set name="pascore-score">100</set>
		<set name="padeath-death">100</set>
		<set name="palosepoint-losepoint">100</set>
		<set name="pacompensate-compensate">100</set>
		<set name="paendmap-endmap">100</set>
		<set name="pasaybold-sb">60</set>
		<set name="parename-rename">80</set>
		<set name="paforceallspec-forceallspec">100</set>
		<set name="pashock-shock">100</set>
		<set name="paburn-burn">100</set>
		<set name="paexplode-explode">100</set>
		<set name="paenforce-enforce">100</set>
		<!-- Next two commands are disabled by default -->
		<set name="paretaliate-retaliate">100</set>
<!--  
		The next commands do not need any serverside mods. Carefull, these control the core of your server
-->
		<set name="pamaprestart-maprestart">100</set>
		<set name="paset">100</set>
		<set name="paget">100</set>
		<set name="paexec">100</set>
	</settings>
</configuration>
